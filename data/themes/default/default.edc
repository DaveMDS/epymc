         }
         program { name: "hide_done"; /* API */
            action: SIGNAL_EMIT "emc,dialog,hide,done" "";
         }
         program { name: "show_title"; /* API */
            signal: "emc,dialog,title,show";
            source: "emc";
            action: STATE_SET "visible" 0.0;
            target: "emc.text.title";
