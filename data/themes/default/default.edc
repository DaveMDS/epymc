         part { name: "topbar.clock";
            type: TEXT;
            clip_to: "topbar.clip";
            scale: 1;
            description { state: "default" 0.0;
               color: FONT_COLOR;
               rel1 {
                  to: "topbar.clip";
               }
               rel2 {
                  to: "topbar.clip";
                  offset: -40 0;
               }
               text {
                  font: "Aclonica";
                  size: FONT_SIZE_BIG;
                  align: 1.0 0.7;
               }
            }
